<div class="container p-4 mx-auto">
    
    <table class="table table-sm table-striped table-bordered table-hover">
    
    <h3>
      <small class="text-muted">Tabla de Goleo</small>
    </h3> 
    
      <thead class="table-success">
        <tr align="center">
          <th scope="col">#</th>

          <th scope="col">Equipo</th>
           <th scope="col">Foto</th>
    
          <th scope="col">Goles</th>
          <th scope="col">Nombre</th>
          <th scope="col">Apellido</th>
          <th scope="col">Equipo</th>
          <th scope="col">Dorsal</th>
          <th scope="col">Torneo</th>
    
        </tr>
      </thead>
      <tbody>
          
          
          
    
        <% for(var i=0 ; i < Torneo.length; i++) { %>
          
        <tr align="center">
          <td><%- i+1 %></td>
          <td><%- torneo[i].Equipo %></td>
          <td><img src="<%- vistas[i].Foto %>" width="40px" height="40px" class="img-thumbnail"></img></td>

          <td><%- torneo[i].Goles %></td>
          <td><%- torneo[i].Nombre %></td>
          <td><%- torneo[i].Apellido %></td>
          <td><%- torneo[i].Equipo %></td>
          <td><%- torneo[i].Torneo %></td>
    
        </tr>
        <% } %>

      </tbody>
    </table>
    
    </div>
    
    
    
    
    <a name="resuljornada"></a>
    
    <%- include('menuinterno') %>

    
    
    
    <div class="container p-1 mx-auto">
    
    <table class="table table-sm table-striped table-bordered table-hover">
        <h3>
      <small class="text-muted">Tabla de Resultados por Jornada</small>
    </h3>
      <thead class="table-success">
        <tr align="center">
          <th scope="col">J</th>
          <th scope="col">Esc</th>
          <th scope="col">Equipo</th>
          <th scope="col">GF</th>
          <th scope="col">GC</th>
          <th scope="col">Rival</th>
                <th scope="col">Escudo R.</th>
    
                <th scope="col">Pts</th>
    
          <th scope="col">Fecha</th>
          <th scope="col">Hora</th>
    
        </tr>
      </thead>
      <tbody>
          
          
          
    
          {{#each j1}}
          
        <tr align="center">
          <td>{{Jornada}}</td>
    
          <td><img src="{{Escudolc}}" width="40px" height="40px" class="img-thumbnail"></img></td>
          <td>{{Equipolc}}</td>
          <td>{{GF}}</td>
          <td>{{GC}}</td>
          <td>{{Nombre_Equipo}}</td>
                <td><img src="{{Escudo}}" width="40px" height="40px" class="img-thumbnail"></img></td>
    
                <td>{{Puntos}}</td>
    
          <td>{{Fecha}}</td>
          <td>{{hora}}</td>
    
        </tr>
      {{/each}}
        
      </tbody>
    </table>
    
    
    
    <a name="resulequipos"></a>
    <%- include('menuinterno') %>

    
    
    
    </div>
    
    
